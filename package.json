{
    "name": "vue-configs-ts",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "types": "vue-tsc",
        "types:watch": "vue-tsc --watch",
        "dev": "vite",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint:prettier": "npx prettier --write .",
        "lint:stylelint": "npx stylelint --fix .",
        "lint:eslint": "npx eslint --fix .",
        "lint:all": "npm run lint:prettier && npm run lint:stylelint && npm run lint:eslint"
    },
    "dependencies": {
        "pinia": "^2.1.7",
        "vue": "^3.4.11",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.18.1",
        "@typescript-eslint/parser": "^6.18.1",
        "@vitejs/plugin-vue": "^5.0.3",
        "autoprefixer": "^10.4.16",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-vue": "^9.20.0",
        "lint-staged": "^15.2.0",
        "postcss-html": "^1.6.0",
        "prettier": "^3.2.1",
        "stylelint": "^16.1.0",
        "stylelint-config-standard-vue": "^1.0.0",
        "typescript": "^5.3.3",
        "vite": "^5.0.11",
        "vue-eslint-parser": "^9.4.0",
        "vue-tsc": "^1.8.27"
    }
}
